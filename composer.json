{
    "name": "sandersyao/graphql-swoole",
    "type": "project",
    "description": "graphql service on swoole",
    "keywords": [
        "graphql",
        "swoole"
    ],
    "license": "MIT",
    "require": {
        "php": ">7.1",
        "ext-swoole": ">4.0",
        "webonyx/graphql-php": "^0.13.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "phpstan/phpstan": "^0.11"
    },
    "autoload": {
        "psr-4": {
            "App\\": "apps/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
